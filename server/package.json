{
    "name": "DiveClubResources",
    "version": "2.0.0",
    "license": "MIT",
    "description": "Server",
    "type": "module",
    "scripts": {
        "lint": "eslint js",
        "simulation": "node server/src/server.js -p 8000 -c example.cfg -d -s"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cdot/DiveClubResources.git"
    },
    "engines": {
        "node": "18.19.0",
        "npm": "10.2.3"
    },
    "author": "Crawford Currie",
    "bugs": {
        "url": "https://github.com/cdot/DiveClubResources/issues"
    },
    "homepage": "https://github.com/cdot/DiveClubResources",
    "devDependencies": {
        "eslint": ">=9.39.1"
    },
    "dependencies": {
        "body-parser": "^2.2.1",
        "cors": "^2.8.5",
        "ds18b20-raspi": "^0.3.6",
        "express": "^5.2.1",
        "fs-extra": "^11.3.2",
        "node-dht-sensor": "^0.4.5",
        "posix-getopt": "^1.2.1"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "parserOptions": {
            "ecmaVersion": 13
        },
        "env": {
            "shared-node-browser": true,
            "jquery": true,
            "es6": true,
            "amd": true
        },
        "globals": {
            "requirejs": "writable",
            "Mocha": "writable"
        },
        "rules": {
            "arrow-parens": 0,
            "camelcase": 0,
            "class-methods-use-this": 0,
            "curly": 0,
            "dot-notation": 1,
            "max-classes-per-file": 0,
            "max-params": 0,
            "new-cap": 0,
            "no-alert": 0,
            "no-console": 0,
            "no-debugger": 0,
            "no-eval": 0,
            "no-fallthrough": 0,
            "no-loop-func": 1,
            "no-mixed-spaces-and-tabs": 0,
            "no-new": 0,
            "no-process-env": 0,
            "no-redeclare": [
                "error",
                {
                    "builtinGlobals": true
                }
            ],
            "no-trailing-spaces": 0,
            "no-underscore-dangle": 0,
            "no-unused-vars": 0,
            "no-useless-escape": 1,
            "no-use-before-define": 0,
            "prefer-promise-reject-errors": 0,
            "quotes": 0,
            "strict": 0
        }
    }
}
